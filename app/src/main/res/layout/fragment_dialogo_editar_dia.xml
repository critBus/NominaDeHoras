        <TextView
            android:id="@+id/tvTarde"
            android:layout_width="80dp"
            android:layout_height="40dp"

            android:layout_marginLeft="36dp"
            android:text="Tarde"
            android:textSize="20sp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="89dp"
            app:layout_constraintTop_toBottomOf="@+id/TBDialogoAceptarAdveertencia" />



        <android.support.design.widget.TextInputLayout
            android:id="@+id/cTS"
            android:layout_width="118dp"
            android:layout_height="45dp"

            android:layout_marginLeft="31dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="13dp"
            app:layout_constraintLeft_toRightOf="@+id/cTE"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cME">


            <maskedEditText.MaskedEditText
                android:id="@+id/metTS"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"

                android:hint="Salida"
                android:inputType="phone"


                android:singleLine="true"
                android:textSize="10sp"

                android:typeface="monospace"
                mask:allowed_chars="1234567890"
                mask:mask="##:##"
                tools:layout_editor_absoluteX="71dp"
                tools:layout_editor_absoluteY="54dp" />
        </android.support.design.widget.TextInputLayout>
        <android.support.design.widget.TextInputLayout
            android:id="@+id/cTE"
            android:layout_width="118dp"
            android:layout_height="46dp"
            app:layout_constraintLeft_toRightOf="@+id/tvTarde"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="13dp"
            app:layout_constraintTop_toBottomOf="@+id/cMS">

            <maskedEditText.MaskedEditText
                android:id="@+id/metTE"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:hint="Entrada"
                android:inputType="phone"
                android:singleLine="true"
                android:textSize="10sp"


                android:typeface="monospace"


                mask:allowed_chars="1234567890"
                mask:mask="##:##" />


        </android.support.design.widget.TextInputLayout>


        <TextView
            android:id="@+id/tvManana"
            android:layout_width="80dp"
            android:layout_height="40dp"

            android:layout_marginLeft="36dp"

            android:layout_marginTop="32dp"
            android:text="MaÃ±ana"
            android:textSize="20sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/TBDialogoAceptarAdveertencia" />

        <android.support.design.widget.TextInputLayout
            android:id="@+id/cMS"
            android:layout_width="118dp"
            android:layout_height="45dp"

            android:layout_marginLeft="31dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="13dp"
            app:layout_constraintLeft_toRightOf="@+id/cME"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/TBDialogoAceptarAdveertencia">


            <maskedEditText.MaskedEditText
                android:id="@+id/vControl2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"

                android:hint="Salida"
                android:inputType="phone"


                android:singleLine="true"
                android:textSize="10sp"

                android:typeface="monospace"
                mask:allowed_chars="1234567890"
                mask:mask="##:##"
                tools:layout_editor_absoluteX="71dp"
                tools:layout_editor_absoluteY="54dp" />
        </android.support.design.widget.TextInputLayout>
        <android.support.design.widget.TextInputLayout
            android:id="@+id/cME"
            android:layout_width="118dp"
            android:layout_height="46dp"
            app:layout_constraintLeft_toRightOf="@+id/tvManana"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="13dp"
            app:layout_constraintTop_toBottomOf="@+id/TBDialogoAceptarAdveertencia">

            <maskedEditText.MaskedEditText
                android:id="@+id/vControl"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:hint="Entrada"
                android:inputType="phone"
                android:singleLine="true"
                android:textSize="10sp"


                android:typeface="monospace"


                mask:allowed_chars="1234567890"
                mask:mask="##:##" />


        </android.support.design.widget.TextInputLayout>


    </android.support.constraint.ConstraintLayout>
